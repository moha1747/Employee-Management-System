      employeeData.firstName !== \"\" &&\n      employeeData.lastName !== \"\" &&\n      employeeData.email !== \"\" &&\n      employeeData.salary !== \"\" &&\n      employeeData.location !== \"\" &&\n      employeeData.position !== \"\"\n    ) {\n      /**If id is present in the parameter, it should update else it should save */\n      if (id) {\n        EmployeeService.updateEmployee(id, employeeData)\n          .then(navigate(\"/employee\"))\n          .catch((e) => console.log(e));\n      } else {\n        EmployeeService.saveEmployee(employeeData)\n          .then(navigate(\"/employee\"))\n          .catch((e) => console.log(e));\n      }\n    } else {\n      alert(\"Please fill in the blanks\");\n    }\n  }\n\n  function tile() {\n    if (id) {\n      return \"Update Employee\";\n    } else {\n      return \"Add Employee\";\n    }\n  }\n  useEffect(() => {\n    if (id) {\n      EmployeeService.getEmployeeById(id)\n        .then((res) => {\n          setFirstName(res.data.firstName);\n          setLastName(res.data.lastName);\n          setEmail(res.data.email);\n          setLocation(res.data.location);\n          setPosition(res.data.position);\n          setSalary(res.data.salary);\n        })\n        .catch((e) => console.log(e));\n    }\n  }, []);\n\n  return (\n    <div>\n      <div className=\"container mt-5\">\n        <div className=\"row\">\n          <div className=\"card col-md-6 offset-md-3\">\n            <h2 className=\"text-center\">{tile()}</h2>\n            <div className=\"card-body\">\n              <form>\n                <div className=\"form-group mb-2\">\n                  <input\n                    className=\"form-control\"\n                    value={firstName}\n                    onChange={(e) => setFirstName(e.target.value)}\n                    type=\"text\"\n                    placeholder=\"Enter First Name\"\n                  />\n                </div>\n                <div className=\"form-group mb-2\">\n                  <input\n                    className=\"form-control\"\n                    value={lastName}\n                    onChange={(e) => setLastName(e.target.value)}\n                    type=\"text\"\n                    placeholder=\"Enter Last Name\"\n                  />\n                </div>\n                <div className=\"form-group mb-2\">\n                  <input\n                    className=\"form-control\"\n                    value={email}\n                    onChange={(e) => setEmail(e.target.value)}\n                    type=\"email\"\n                    placeholder=\"Enter Email\"\n                  />\n                </div>\n                <div className=\"form-group mb-2\">\n                  <input\n                    className=\"form-control\"\n                    value={location}\n                    onChange={(e) => setLocation(e.target.value)}\n                    type=\"location\"\n                    placeholder=\"Enter Location\"\n                  />\n                </div>\n                <div className=\"form-group mb-2\">\n                  <input\n                    className=\"form-control\"\n                    value={position}\n                    onChange={(e) => setPosition(e.target.value)}\n                    type=\"Position\"\n                    placeholder=\"Enter Position\"\n                  />\n                </div>\n                <div className=\"form-group mb-2\">\n                  <input\n                    className=\"form-control\"\n                    value={salary}\n                    onChange={(e) => setSalary(e.target.value)}\n                    type=\"salary\"\n                    placeholder=\"Enter Salary\"\n                  />\n                </div>\n                <button\n                  onClick={(e) => saveEmployee(e)}\n                  className=\"btn btn-success\"\n                >\n                  Save\n                </button>{\" \"}\n                <Link to={\"/employee\"} className=\"btn btn-danger\" href=\"\">\n                  Cancel\n                </Link>\n              </form>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default AddEmployeeComponent;\n","/Users/ayeebosss/moha/employee-system/employeemanagementfrontend/src/service/EmployeeService.js",[],[],"/Users/ayeebosss/moha/employee-system/employeemanagementfrontend/src/component/FooterComponent.js",[],[],"/Users/ayeebosss/moha/Employee-Management-System/employeemanagementfrontend/src/index.js",[],[],"/Users/ayeebosss/moha/Employee-Management-System/employeemanagementfrontend/src/App.js",[],[],"/Users/ayeebosss/moha/Employee-Management-System/employeemanagementfrontend/src/component/HeaderComponent.js",[],[],"/Users/ayeebosss/moha/Employee-Management-System/employeemanagementfrontend/src/component/ListEmployeeComponent.js",[],[],"/Users/ayeebosss/moha/Employee-Management-System/employeemanagementfrontend/src/component/AddEmployeeComponent.js",["101"],[],"/Users/ayeebosss/moha/Employee-Management-System/employeemanagementfrontend/src/component/FooterComponent.js",[],[],"/Users/ayeebosss/moha/Employee-Management-System/employeemanagementfrontend/src/service/EmployeeService.js",[],[],{"ruleId":"102","severity":1,"message":"103","line":9,"column":25,"nodeType":"104","endLine":9,"endColumn":65},{"ruleId":"105","severity":1,"message":"106","line":1,"column":8,"nodeType":"107","messageId":"108","endLine":1,"endColumn":13},{"ruleId":"102","severity":1,"message":"109","line":45,"column":33,"nodeType":"104","endLine":45,"endColumn":111},{"ruleId":"110","severity":1,"message":"111","line":19,"column":97,"nodeType":"112","messageId":"113","endLine":19,"endColumn":99},{"ruleId":"114","severity":1,"message":"115","line":53,"column":8,"nodeType":"116","endLine":53,"endColumn":10,"suggestions":"117"},{"ruleId":"114","severity":1,"message":"115","line":65,"column":6,"nodeType":"116","endLine":65,"endColumn":8,"suggestions":"118"},{"ruleId":"